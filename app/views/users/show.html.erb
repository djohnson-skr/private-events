<h1>Hello <%= @user.name %>, see your created events below</h1>
<% @user.created_events.each do |event| %>
  <%= link_to event.name, event %><br><br>
<% end %>

<h1>Events RSVPed to</h1>
<h3>Upcoming Events:</h3>
<% upcoming_events = @user.attended_events.upcoming_events %>
<% upcoming_events.each do |event| %>
  <%= event.name %><br>
<% end %>

<h3>Past Events:</h3>
<% past_events = @user.attended_events.past_events %>
<% past_events.each do |event| %>
  <%= event.name %><br>
<% end %>


